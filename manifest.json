{
   "background": {
      "scripts": [ "noteslib/util.js", "noteslib/ContextMenuCreator.js", "noteslib/DataManager.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "nortesdisplayer.html",
      "default_title": "note's maker"
   },
   "content_scripts": [ {
      "css": [ "assets/styles/style.css" ],
      "js": [ "noteslib/NotesContent.js" ],
      "matches": [ "http://*/*" ]
   } ],
   "description": " create note's",
   "icons": {
      "128": "icon_128.png",
      "16": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "note's maker",
   "permissions": [ "storage", "background", "activeTab", "tabs", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2"
}
